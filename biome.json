{
   "$schema": "https://biomejs.dev/schemas/1.3.3/schema.json",
   "formatter": {
      "enabled": true,
      "indentStyle": "space",
      "indentWidth": 3
   },
   "linter": {
      "enabled": true,
      "rules": {
         "recommended": true,
         "suspicious": {
            "noConsoleLog": "warn"
         }
      }
   },
   "organizeImports": {
      "enabled": false
   }
}
